debug = require 'debug',

run = namespace -> namespace "return",


count = tuple -> ((n, head, tail) -> head == () ? n ; self(n+1, tail))(0, tuple),


map = f -> tuple -> run {

        _map: (head, tail) -> head != () ? (f head, _map tail),
        return: _map tuple

    },


factorial = n -> run {

        fac: n -> n == 0 ? 1 ; n * fac(n-1),
        return: fac n
        
}    
    